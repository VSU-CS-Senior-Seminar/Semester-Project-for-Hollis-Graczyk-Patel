<p id="notice"><%= notice %></p>

<h1>Listing Business Messages</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_messages.each do |business_message| %>
      <tr>
        <td><%= business_message.title %></td>
        <td><%= business_message.content %></td>
        <td><%= business_message.user %></td>
        <td><%= link_to 'Show', business_message %></td>
        <td><%= link_to 'Edit', edit_business_message_path(business_message) %></td>
        <td><%= link_to 'Destroy', business_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business message', new_business_message_path %>
